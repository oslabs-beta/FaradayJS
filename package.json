    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "electron-is-dev": "^2.0.0",
    "express": "^4.17.1",
    "pg": "^8.7.1",
    "pg-promise": "^10.11.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0"
  },
