{
  "name": "ElectronTest",
  "version": "1.0.0",
  "description": "Test for Electron",
  "main": "electron/electron.ts",
  "homepage": "./",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server/server.js",
    "build": "cross-env NODE_ENV=production webpack",
    "dev": "concurrently \"cross-env NODE_ENV=development webpack serve --hot --open\" \"nodemon server/server.js\"",
    "watch": "webpack --config webpack.config.js --watch",
    "electron-builder": "electron-builder",
    "electron-build": "npm run build && npm run electron-builder",
    "electron-barestart": "electron .",
    "electron-start": "concurrently \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "author": "",
  "license": "ISC",
